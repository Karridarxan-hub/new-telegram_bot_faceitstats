# Load Testing Configuration
# Copy this to .env for load testing

# Telegram Bot (use test bot token if available)
TELEGRAM_BOT_TOKEN=your_test_bot_token_here

# FACEIT API (required for testing)
FACEIT_API_KEY=your_faceit_api_key_here

# Redis Configuration for Load Testing
REDIS_URL=redis://localhost:6379
REDIS_PASSWORD=
REDIS_MAX_CONNECTIONS=50

# Database Configuration (optional for load testing)
DATABASE_URL=postgresql://user:password@localhost:5432/faceit_test

# Cache TTL Settings (optimized for testing)
CACHE_TTL_PLAYER=300
CACHE_TTL_MATCH=120
CACHE_TTL_STATS=600

# Performance Settings
DB_POOL_SIZE=30
DB_POOL_OVERFLOW=50
DB_POOL_TIMEOUT=30

# Queue Settings for Load Testing
QUEUE_MAX_WORKERS=8
QUEUE_CONNECTION_POOL_SIZE=30

# Logging
LOG_LEVEL=INFO